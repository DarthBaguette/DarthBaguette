    "from sympy import *\n",
    "\n",
    "rho, theta, phi = symbols('rho theta phi')\n",
    "\n",
    "f = rho**2 * sin(phi)\n",
    "\n",
    "res = integrate(f, (rho, 0, 2*cos(phi)), (phi, pi/4, pi/2), (theta, 0, 2*pi))\n",
    "print(res)\n"
   ]
